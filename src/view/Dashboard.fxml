<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="ui.DashboardController"
      spacing="10" alignment="CENTER" style="-fx-padding:20;">
    <Label fx:id="welcomeLabel" text="Welcome!" style="-fx-font-size: 16px;"/>
    <Button fx:id="courseManagementButton" text="Manage Courses" onAction="#handleCourseManagement"/>
    <Button fx:id="enrollmentButton" text="Enroll Courses" onAction="#handleEnrollment"/>
    <Button fx:id="transcriptButton" text="View Transcript" onAction="#handleTranscript"/>
    <Button fx:id="payTuitionButton" text="Pay Tuition" onAction="#handlePayTuition"/>
    <Button fx:id="gradeManagementButton" text="Manage Grades" onAction="#handleGradeManagement"/>
    <Button fx:id="logoutButton" text="Logout" onAction="#handleLogout"/>
</VBox>