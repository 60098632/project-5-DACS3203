<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="ui.TranscriptController"
      spacing="10" alignment="CENTER" prefWidth="600" prefHeight="400">
    <Label text="Transcript" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>
    <TextArea fx:id="transcriptArea" prefWidth="550" prefHeight="300" editable="false" wrapText="true"/>
    <Button text="Back" onAction="#handleBack"/>
</VBox>